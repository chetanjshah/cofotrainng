            steps {
                echo 'Build World from GIT'
            }
        }
		stage('Test') {
            steps {
                echo 'Test  World from GIT'
            }
        }
		stage('Deploy') {
            steps {
                echo 'Deploy World from GIT'
            }
        }
    }
}
